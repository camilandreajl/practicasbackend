{
  "name": "api",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "exports": "./dist/index.js",
  "scripts": {
    "generate_build": "cosmo && npx prisma generate && prettier --config .prettierrc prisma/generated/graphql/**/*.ts --write && next build",
    "cosmo": "cosmo && prettier --config .prettierrc prisma/generated/graphql/**/*.ts --write",
    "generate": "yarn aurora && npx prisma generate && yarn cosmo",
    "migrate-dev": "yarn aurora && prisma migrate dev",
    "migrate-deploy": "yarn aurora && npx prisma migrate deploy",
    "compile": "rimraf ./dist && tsc",
    "build": "esbuild index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "start": "ts-node index.ts"
  },
  "dependencies": {
    "@as-integrations/aws-lambda": "^2.0.1",
    "@aws-sdk/client-secrets-manager": "^3.477.0",
    "@prisma/client": "^4.9.0",
    "aws-sdk": "^2.1237.0",
    "dataloader": "^2.2.2",
    "nanoid": "3.0.0",
    "node-fetch": "2.6.1",
    "nodemailer": "^6.9.3"
  },
  "devDependencies": {
    "@apollo/server": "^4.0.1",
    "@prevalentware/prisma-cosmo": "^0.1.16",
    "@types/node": "^18.11.3",
    "@types/nodemailer": "^6.4.9",
    "esbuild": "^0.15.12",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
